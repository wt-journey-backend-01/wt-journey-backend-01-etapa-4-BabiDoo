{
  "name": "depart-pm",
  "version": "1.0.0",
  "description": "Etapa 3 DepartPM - WebTech Journey Backend",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "npm --watch server.js",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "db:reset": "knex migrate:rollback --all && npm run db:setup"
  },
  "author": "Barbara Dorneles",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "knex": "^3.1.0",
    "pg": "^8.16.3"
  }
}
